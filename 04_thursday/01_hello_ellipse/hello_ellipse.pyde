size(640, 360)  # set window size
background(200) # set background color (grayscale)

stroke(0, 53, 138) # set outline (stroke) color (RGB)
strokeWeight(4)   # set stroke width

fill(66, 135, 245, 191) # set shape interior color (RGBA)
# no fourth argument means 100% opacity
# 255: 100% opacity, 191: 75% opacity, 63: 25% opacity

ellipse(width/2, height/2, 60, 60) # draw an ellipse
